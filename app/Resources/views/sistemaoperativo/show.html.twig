{% extends 'base.html.twig' %}

{% block body %}
    <h1>Sistemaoperativo</h1>

    <table>
        <tbody>
            <tr>
                <th>Descripcion</th>
                <td>{{ sistemaOperativo.descripcion }}</td>
            </tr>
            <tr>
                <th>Bytes</th>
                <td>{{ sistemaOperativo.bytes }}</td>
            </tr>
            <tr>
                <th>Id</th>
                <td>{{ sistemaOperativo.id }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('sistemaoperativo_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('sistemaoperativo_edit', { 'id': sistemaOperativo.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
