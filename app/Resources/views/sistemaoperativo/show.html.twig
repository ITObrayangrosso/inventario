{% extends 'base.html.twig' %}

{% block body %}
    <h1>Sistema operativo</h1>

    <table class="table table-striped table-bordered table-hover">
        <tbody>
            <tr>
                <th>Descripcion</th>
                <td>{{ sistemaOperativo.descripcion }}</td>
            </tr>
            <tr>
                <th>Bytes</th>
                <td>{{ sistemaOperativo.bytes }}</td>
            </tr>
        </tbody>
    </table>
    <a class="btn boton_crear" href="{{ path('sistemaoperativo_edit', { 'id': sistemaOperativo.id }) }}">Editar</a>
    <a href="{{ path('sistemaoperativo_index') }}">Volver a la lista</a>
{% endblock %}
