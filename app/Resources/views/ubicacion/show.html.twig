{% extends 'base.html.twig' %}

{% block body %}
    <h1>Ubicacion</h1>

    <table class="table table-striped table-bordered table-hover">
        <tbody>
            <tr>
                <th>Descripcion</th>
                <td>{{ ubicacion.descripcion }}</td>
            </tr>
        </tbody>
    </table>
    <a class="btn boton_crear" href="{{ path('ubicacion_edit', { 'id': ubicacion.id }) }}">Editar</a>
    <a href="{{ path('ubicacion_index') }}">Volver a la lista</a>

{% endblock %}
