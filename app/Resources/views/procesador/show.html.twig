{% extends 'base.html.twig' %}

{% block body %}
    <h1>Procesador</h1>

    <table class="table table-striped table-bordered table-hover">
        <tbody>
            <tr>
                <th>Descripcion</th>
                <td>{{ procesador.descripcion }}</td>
            </tr>
        </tbody>
    </table>
    <a class="btn boton_crear" href="{{ path('procesador_edit', { 'id': procesador.id }) }}">Editar</a>
    <a href="{{ path('procesador_index') }}">Volver a la lista</a>        
{% endblock %}
